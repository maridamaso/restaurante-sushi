{
    "version": 2,
    "name": "restaurante",
    "builds": [
      {
        "src": "back-end/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "front-end/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/back-end/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/front-end/$1"
      }
    ]
  }
  